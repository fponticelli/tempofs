{
    "name": "tempo",
    "version": "0.0.1",
    "description": "Tempo",
    "main": "index.js",
    "scripts": {
        "postinstall": "dotnet tool restore && npm i",
        "watch": "dotnet fable src/Tempo.Demo --watch --run webpack serve",
        "prebuild": "npx del-cli **/output && dotnet fable clean --yes",
        "build": "dotnet fable src/Tempo.Demo --run webpack build --mode production",
        "pack": "npm run prebuild && dotnet build -c Release && dotnet pack -c Release --output nupkgs",
        "deploy": "npm run build && gh-pages -d output"
    },
    "repository": "git@github.com:fponticelli/tempofs.git",
    "author": "Franco Ponticelli",
    "private": true,
    "dependencies": {
        "@babel/polyfill": "^7.8.7",
        "core-js": "^3.12.1",
        "monaco-editor": "^0.25.2"
    },
    "devDependencies": {
        "@babel/core": "^7.14.0",
        "@babel/plugin-external-helpers": "^7.12.13",
        "@babel/plugin-transform-runtime": "^7.13.15",
        "@babel/preset-env": "^7.14.1",
        "autoprefixer": "^10.2.5",
        "babel-loader": "^8.2.2",
        "copy-webpack-plugin": "^8.1.1",
        "css-loader": "^5.2.4",
        "cssnano": "^5.0.2",
        "del-cli": "^3.0.1",
        "fable-compiler": "^2.13.0",
        "fable-loader": "^2.1.9",
        "file-loader": "^6.2.0",
        "gh-pages": "^3.2.0",
        "html-webpack-plugin": "^5.3.1",
        "monaco-editor-webpack-plugin": "^4.0.0",
        "postcss": "^8.3.5",
        "postcss-loader": "^6.1.0",
        "postcss-preset-env": "^6.7.0",
        "remotedev": "^0.2.9",
        "style-loader": "^2.0.0",
        "tailwindcss": "^2.2.4",
        "webpack": "^5.37.0",
        "webpack-cli": "^4.7.0",
        "webpack-dev-server": "^3.11.2"
    }
}