{
    "name": "tempo",
    "version": "0.0.1",
    "description": "Tempo",
    "main": "index.js",
    "scripts": {
        "postinstall": "dotnet tool restore && npm i",
        "watch": "dotnet fable src/Tempo.Demo --watch --run webpack serve",
        "prebuild": "npx del-cli **/output && dotnet fable clean --yes",
        "build": "dotnet fable src/Tempo.Demo --run webpack build --mode production",
        "pack": "npm run prebuild && dotnet build -c Release && dotnet pack -c Release --output nupkgs",
        "deploy": "npm run build && gh-pages -d output"
    },
    "repository": "git@github.com:fponticelli/tempofs.git",
    "author": "Franco Ponticelli",
    "private": true,
    "dependencies": {
        "@babel/polyfill": "^7.8.7",
        "core-js": "^3.15.2",
        "monaco-editor": "^0.26.1"
    },
    "devDependencies": {
        "@babel/core": "^7.14.6",
        "@babel/plugin-external-helpers": "^7.14.5",
        "@babel/plugin-transform-runtime": "^7.14.5",
        "@babel/preset-env": "^7.14.7",
        "autoprefixer": "^10.3.1",
        "babel-loader": "^8.2.2",
        "copy-webpack-plugin": "^9.0.1",
        "css-loader": "^6.0.0",
        "cssnano": "^5.0.6",
        "del-cli": "^4.0.1",
        "fable-compiler": "^2.13.0",
        "fable-loader": "^2.1.9",
        "file-loader": "^6.2.0",
        "fs-extra": "^10.0.0",
        "gh-pages": "^3.2.3",
        "html-webpack-plugin": "^5.3.2",
        "monaco-editor-webpack-plugin": "^4.1.1",
        "postcss": "^8.3.5",
        "postcss-loader": "^6.1.1",
        "postcss-preset-env": "^6.7.0",
        "remotedev": "^0.2.9",
        "style-loader": "^3.1.0",
        "tailwindcss": "^2.2.4",
        "webpack": "^5.45.0",
        "webpack-cli": "^4.7.2",
        "webpack-dev-server": "^3.11.2"
    }
}
